

<!DOCTYPE html>

<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width" initial-scale="1.0" />
    <title>Pharmacist Dashboard</title>
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200">
    <link rel="stylesheet" href="~/css/PharmacistDashboard.css">
</head>
<body>
    <aside class="sidebar">
        <div class="logo">
            <img src="~/css/Chris Hani Brand.jpg" alt="logo">

        </div>
        <ul class="links">
            <h4>Pharmacist Dashboard</h4>

            <li>
                <a class="nav-link text-dark" asp-area="Identity" asp-page="/Account/Register"> <i class="bi bi-pencil-square"> </i> Register</a>
            </li>

            <li>

                <span class="material-symbols-outlined">medication</span>
                <a href='@Url.Action("", "")'>Medication</a>
            </li>
            <li>
                <span class="material-symbols-outlined">medication</span>
                <a href='@Url.Action("", "")'>Orders</a>
            </li>


            <li>
                <span class="material-symbols-outlined">group</span>
                <a href="@Url.Action("", "")">Make Orders</a>
            </li>

            <li>
                <span class="material-symbols-outlined">book</span>
                <a href="@Url.Action("", "")">Prescription</a>
            </li>

            <li>
                <form class="form-inline" asp-area="Identity" asp-page="/Account/Logout" asp-route-returnUrl="@Url.Action("Index", "Home", new { area = "" })">
                    <a href='@Url.Action("Index", "Home", new { area = "" })' class="nav-link btn btn-link text-dark">Logout</a>
                </form>
            </li>
        </ul>
    </aside>

    <div>
        @RenderBody()
    </div>
</body>
</html>

