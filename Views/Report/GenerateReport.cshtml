@model E_PRESCRIBING_SYSTEM.Models.PharmacistReportViewModel

@{
    ViewData["Title"] = "Generate Pharmacist Report";
    Layout = "~/Views/Shared/_PharmacistLayout.cshtml"; // Adjust the layout as needed
}

<h2>Generate Pharmacist Report</h2>

<form asp-action="GenerateReport" method="post">
    <div class="form-group">
        <label for="startDate">Start Date</label>
        <input type="date" name="startDate" class="form-control" required />
    </div>

    <div class="form-group">
        <label for="endDate">End Date</label>
        <input type="date" name="endDate" class="form-control" required />
    </div>

    <button type="submit" class="btn btn-primary">Generate Report</button>
</form>
